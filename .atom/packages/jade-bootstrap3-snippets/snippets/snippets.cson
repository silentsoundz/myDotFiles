	'bs-accordion':
		'prefix': 'bs-accordion'
		'body': '''
	#${1:accordion-id}.panel-group
			.panel.panel-default
				.panel-heading
					h4.panel-title
						a.accordion-toggle.collapsed(data-toggle="collapse", data-parent="#${1:accordion-id}", href="#collapseOne")
							| Collapsible Group Item #1
				#collapseOne.panel-collapse.collapse
					.panel-body
						| Panel body
			.panel.panel-default
				.panel-heading
					h4.panel-title
						a.accordion-toggle.collapsed(data-toggle="collapse", data-parent="#${1:accordion-id}", href="#collapseTwo")
							| Collapsible Group Item #2
				#collapseTwo.panel-collapse.in
					.panel-body
						| Panel body
	'''
	'bs-alert-danger':
		'prefix': 'bs-alert-danger'
		'body': '''
	.alert.alert-success(role="alert")
		button.close(type="button", data-dismiss="alert")
			span(aria-hidden="true") &times;
			span.sr-only Close
		strong ${1:Title!}
		| ${2:Alert body ...}
	'''
